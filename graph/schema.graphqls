# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Query {
  users(limit:Int=10, offset:Int=0): [GetUser!]!
}

type GetUser {
  id: String
  created_at: String
  updated_at: String
  first_name: String
  last_name: String
  phone_number: String!
  long: Float
  lat: Float
  user_type: Int
}

type User {
  first_name: String
  last_name: String
  phone_number: String!
  is_verified: Boolean!
  long: Float
  lat: Float
  user_type: Int
}
